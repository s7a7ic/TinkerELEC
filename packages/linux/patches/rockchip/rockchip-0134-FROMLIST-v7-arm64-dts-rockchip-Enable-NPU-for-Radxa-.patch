+++ b/arch/arm64/boot/dts/rockchip/rk3588s-rock-5c.dts
@@ -444,6 +444,10 @@ &pd_gpu {
 	domain-supply = <&vdd_gpu_s0>;
 };
 
+&pd_npu {
+	domain-supply = <&vdd_npu_s0>;
+};
+
 &pinctrl {
 	ethernet {
 		gmac1_rstn: gmac1-rstn {
@@ -504,6 +508,36 @@ &pwm3 {
 	status = "okay";
 };
 
+&rknn_core_0 {
+	npu-supply = <&vdd_npu_s0>;
+	sram-supply = <&vdd_npu_s0>;
