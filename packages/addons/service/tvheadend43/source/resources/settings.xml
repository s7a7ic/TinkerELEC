			<group id="2">
				<setting id="WAIT_FOR_FEINIT" type="boolean" label="30011" help="">
					<level>0</level>
					<default>false</default>
					<control type="toggle"/>
				</setting>
				<setting id="NUM_ADAPTERS" type="integer" label="30012" help="">
					<level>0</level>
					<default>1</default>
					<constraints>
						<minimum>1</minimum>
						<maximum>16</maximum>
					</constraints>
					<dependencies>
						<dependency type="enable">
							<condition operator="is" setting="WAIT_FOR_FEINIT">true</condition>
						</dependency>
					</dependencies>
					<control type="slider" format="integer">
