				<setting id="WORKAROUND_SLEEP" type="boolean" label="30014" help="">
					<level>0</level>
					<default>false</default>
					<control type="toggle"/>
				</setting>
				<setting id="WORKAROUND_SLEEP_TIME" type="integer" label="30015" help="">
