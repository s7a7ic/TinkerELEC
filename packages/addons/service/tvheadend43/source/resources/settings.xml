		<category id="xmltv" label="30000" help="">
			<group id="1" label="30004"/>
			<group id="2">
				<setting id="XMLTV_TYPE" type="string" label="30005" help="">
					<level>0</level>
					<default>NONE</default>
					<constraints>
						<options sort="ascending">
							<option label="NONE">NONE</option>
							<option label="FILE">FILE</option>
							<option label="SCRIPT">SCRIPT</option>
							<option label="WEB">WEB</option>
						</options>
					</constraints>
					<control type="spinner" format="string"/>
				</setting>
				<setting id="XMLTV_LOCATION_FILE" type="path" label="30006" help="">
					<level>0</level>
					<default/>
					<constraints>
						<writable>false</writable>
						<allowempty>true</allowempty>
					</constraints>
					<dependencies>
						<dependency type="enable">
							<condition operator="is" setting="XMLTV_TYPE">1</condition>
						</dependency>
					</dependencies>
					<control type="button" format="file">
						<heading>30006</heading>
					</control>
				</setting>
				<setting id="XMLTV_LOCATION_WEB" type="string" label="30007" help="">
					<level>0</level>
					<default>http://</default>
					<dependencies>
						<dependency type="enable">
							<condition operator="is" setting="XMLTV_TYPE">3</condition>
						</dependency>
					</dependencies>
					<control type="edit" format="string">
						<heading>30007</heading>
					</control>
				</setting>
				<setting id="XMLTV_LOCATION_SCRIPT" type="path" label="30008" help="">
					<level>0</level>
					<default/>
					<constraints>
						<writable>false</writable>
						<allowempty>true</allowempty>
					</constraints>
					<dependencies>
						<dependency type="enable">
							<condition operator="is" setting="XMLTV_TYPE">2</condition>
						</dependency>
					</dependencies>
					<control type="button" format="file">
						<heading>30008</heading>
					</control>
				</setting>
			</group>
		</category>
		<category id="dvb" label="30001" help="">
