function custom_post_commit() {
  echo "Starting revision update of dependant addons"
  # do not change the version, just increment the addon
  export commit_msg="vfs.libarchive: update libarchive to ${pkgver}"
  export commit_body="\n\n- libarchive: ${pkgver}"
  ${progname} vfs.libarchive $(get_pkg_version vfs.libarchive)
}
