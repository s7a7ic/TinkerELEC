function pkg_git_commit() {
  github_repos="moby/moby"
  prefix="docker-"
}

function custom_post_commit() {
  ${progname} cli ${pkgver}
  echo "Starting revision update of dependant addons"
  # do not change the version, just increment the addon
  export commit_msg="docker: update to ${pkgver}"
  export commit_body="\n\n- moby: ${pkgver}"
  export commit_body+="\n- cli: ${pkgver}"
  ${progname} docker $(get_pkg_version docker)
}
