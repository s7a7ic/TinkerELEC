function custom_post_commit() {
  echo "Starting revision update of dependant addons"
  # do not change the version, just increment the addon
  export commit_msg="imagedecoder.raw: update libraw to ${pkgver}"
  export commit_body="\n\n- libraw: ${pkgver}"
  ${progname} imagedecoder.raw $(get_pkg_version imagedecoder.raw)
}
